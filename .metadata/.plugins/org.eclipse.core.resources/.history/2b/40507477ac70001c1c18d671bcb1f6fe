
public class TestaVariaveis {

	public static void main(String[] args) {
		System.out.println("Helo new test");
		
		// int: inteiro
		int idade;
		idade = 32;
		System.out.println(idade);
		
		idade = 30 + 10;
		System.out.println(idade);
		// primeiro, ele faz multiplicação depois a soma
		idade = (7 * 5) + 2;
		
		System.out.println(idade);
	}
	
}
